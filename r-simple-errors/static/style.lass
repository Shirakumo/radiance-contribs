(body
 :font-family sans-serif
 :font-size 16pt
 :margin 0
 :background "#000000"
 (a
  :color "#0088EE"
  :text-decoration none)
 ("a:hover"
  :color "#00EEFF")
 (header
  :background "#151515"
  :color "#EEEEEE"
  :border-bottom 3px solid "#EE2200"
  :margin 20px 0 0 0
  :padding 10px
  (h1
   :margin 0 auto 0 auto
   :max-width 800px))
 (main
  :margin 0 auto 0 auto
  :max-width 800px
  :background "#EEEEEE"
  :padding 10px 20px 20px 20px
  (p
   :margin 0)))
